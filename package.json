{
  "name": "js-dev-env",
  "version": "0.0.1",
  "description": "SIMSpellstone Engine",
  "scripts": {
    "build": "npm-run-all build:clean test lint build:compile",
    "build:clean": "rimraf ./dist && mkdir dist",
    "build:compile": "babel-node buildScripts/build.js",
    "start": "npm-run-all --parallel security-check lint:watch test:watch",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.spec.js\" --watch-extensions html,js,jsx,ts",
    "test:watch": "npm run test -- --watch",
    "inspect": "node-inspector & mocha buildScripts/testSetup.js \"src/**/*.spec.js\" --ui bdd -d -g \"should X then Y\" --debug-brk"
  },
  "author": "Jon Senchyna",
  "license": "MIT",
  "dependencies": {
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chalk": "2.3.2",
    "compression": "1.7.2",
    "cross-env": "5.1.4",
    "eslint": "4.19.1",
    "eslint-plugin-import": "2.11.0",
    "eslint-watch": "3.1.4",
    "file-loader": "^1.1.11",
    "mocha": "5.1.0",
    "npm-run-all": "4.1.2",
    "nsp": "3.2.1",
    "numeral": "2.0.6",
    "rimraf": "2.6.2",
    "url-loader": "^1.0.1",
    "webpack": "4.5.0",
    "webpack-dev-middleware": "3.1.2"
  }
}
